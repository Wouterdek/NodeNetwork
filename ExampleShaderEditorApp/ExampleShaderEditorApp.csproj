<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>netcoreapp3.0;net462</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <ProjectTypeGuids>{60dc8134-eba5-43b8-bcc9-bb4bc16c2548};{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</ProjectTypeGuids>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyTitle>ExampleShaderEditorApp</AssemblyTitle>
    <Product>NodeNetwork</Product>
    <Copyright>Copyright (c) Wouter De Keersmaecker</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <ExtrasEnableWpfProjectSetup>true</ExtrasEnableWpfProjectSetup>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DynamicData" Version="6.6.1.2507" />
    <PackageReference Include="Extended.Wpf.Toolkit" Version="3.4.0" />
    <PackageReference Include="MathNet.Numerics" Version="4.6.0" />
    <PackageReference Include="MathNet.Spatial" Version="0.4.0" />
    <PackageReference Include="OpenGL.Net" Version="0.7.1" />
    <PackageReference Include="OpenGL.Net.CoreProfile" Version="0.7.1" />
    <PackageReference Include="OpenGL.Net.Math" Version="0.7.1" />
    <PackageReference Include="OpenGL.Net.WinForms" Version="0.7.1" />
    <PackageReference Include="ReactiveUI" Version="9.1.4" />
    <PackageReference Include="Splat" Version="5.0.2" />
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
    <PackageReference Include="System.Drawing.Primitives" Version="4.3.0" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
    <PackageReference Include="System.Reactive" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Compatibility" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Core" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Experimental" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Interfaces" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Linq" Version="4.1.3" />
    <PackageReference Include="System.Reactive.PlatformServices" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Providers" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Runtime.Remoting" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Windows.Forms" Version="4.1.3" />
    <PackageReference Include="System.Reactive.Windows.Threading" Version="4.1.3" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="4.5.2" />
    <PackageReference Include="System.Threading.Tasks.Extensions" Version="4.5.2" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\Models\suzanne.obj">
      <SubType>Designer</SubType>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Shaders\fragment.glsl" />
    <EmbeddedResource Include="Resources\Shaders\vertex.glsl" />
    <EmbeddedResource Include="Resources\Models\cube.obj" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\NodeNetworkToolkit\NodeNetworkToolkit.csproj" />
    <ProjectReference Include="..\NodeNetwork\NodeNetwork.csproj" />
  </ItemGroup>
  <ItemGroup>
    <!-- This isn't supposed to be neccessary since we're including the desktop SDK. Once whatever is going on right now gets figured out, this should be removed -->
    <Reference Include="WindowsFormsIntegration" />
  </ItemGroup>
</Project>