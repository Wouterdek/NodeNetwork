<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.0;net462</TargetFrameworks>
    <ProjectTypeGuids>{60dc8134-eba5-43b8-bcc9-bb4bc16c2548};{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</ProjectTypeGuids>
    <AssemblyTitle>NodeNetwork</AssemblyTitle>
    <Product>NodeNetwork</Product>
    <Description>A library with a WPF node editor component based on ReactiveUI</Description>
    <Copyright>Copyright (c) Wouter De Keersmaecker</Copyright>
    <Version>3.1.0.0</Version>
    <AssemblyVersion>3.1.0.0</AssemblyVersion>
    <FileVersion>3.1.0.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <ExtrasEnableWpfProjectSetup>true</ExtrasEnableWpfProjectSetup>
    <Authors>Wouter De Keersmaecker</Authors>
    <PackageIconUrl>https://raw.githubusercontent.com/Wouterdek/NodeNetwork/gh-pages/img/logo.png</PackageIconUrl>
    <PackageProjectUrl>https://www.github.com/wouterdek/nodenetwork</PackageProjectUrl>
    <PackageTags>wpf reactiveui node network editor node-editor graph</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DocumentationFile>bin\$(Configuration)\NodeNetwork.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>$(NoWarn);NU1605</NoWarn>
    <NoWarn>$(NoWarn);NU1701</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
    <PackageReference Include="ReactiveUI.Events.WPF" Version="9.13.1" />
    <PackageReference Include="ReactiveUI.WPF" Version="9.13.1" />
    <PackageReference Include="ReactiveUI" Version="9.13.1" />
  </ItemGroup>
  <ItemGroup>
    <Page Include="Themes\*.xaml" />
    <Page Include="Views\*.xaml" />
    <Page Include="Views\Controls\*.xaml" />
  </ItemGroup>
</Project>